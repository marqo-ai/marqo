indexing: summary
}
field marqo__lexical_category type string {
indexing: index | summary
index: enable-bm25
}
field marqo__filter_category type string {
indexing: attribute | summary
attribute: fast-search
rank: filter
}
field marqo__filter_tags type array<string> {
indexing: attribute | summary
attribute: fast-search
rank: filter
}
field image type string {
indexing: summary
}
field marqo__filter_is_active type byte {
indexing: attribute | summary
attribute: fast-search
rank: filter
}
field price type float {
indexing: summary
}
field rank type int {
indexing: summary
}
field marqo__filter_click_per_day type array<int> {
indexing: attribute | summary
attribute: fast-search
rank: filter
}
field marqo__filter_last_updated type array<float> {
indexing: attribute | summary
attribute: fast-search
rank: filter
}
field marqo__score_modifiers_double_long type tensor<double>(p{}) { indexing: attribute | summary }
field marqo__score_modifiers_float type tensor<float>(p{}) { indexing: attribute | summary }
field marqo__chunks_title type array<string> {
indexing: attribute | summary
}
